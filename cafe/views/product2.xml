<odoo>
    <data>
        <!--        starter-->
        <record id="starter_view_action" model="ir.actions.act_window">
            <field name="name">cafe.starter</field>
            <field name="res_model">cafe.starter</field>
            <field name="view_mode">tree,form</field>
            <field name="help" type="xml">
                <p class="o_view_nocontent_smiling_face">
                    Create Product
                </p>
            </field>
        </record>

        <record id="cafe_starter_form" model="ir.ui.view">
            <field name="name">cafe.starter.form</field>
            <field name="model">cafe.starter</field>
            <field name="arch" type="xml">
                <form string="product">
                    <group>
                        <field name="name"/>
                    </group>
                </form>
            </field>
        </record>

        <record id="cafe_starter_tree" model="ir.ui.view">
            <field name="name">cafe.starter.tree</field>
            <field name="model">cafe.starter</field>
            <field name="arch" type="xml">
                <tree string="product">
                    <field name="name"/>
                </tree>
            </field>
        </record>

    </data>
</odoo>