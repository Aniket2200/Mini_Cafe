<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Sequences for cafe -->
        <record id="seq_cafe_staff" model="ir.sequence">
            <field name="name">Staff</field>
            <field name="code">cafe.staff_member</field>
            <field name="prefix">STF</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequences for Order -->
        <record id="seq_cafe_order" model="ir.sequence">
            <field name="name">Order</field>
            <field name="code">cafe.order</field>
            <field name="prefix">ORD</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>




    </data>
</odoo>